#!/bin/bash

f_export_if_not_defined() {
  if [ -n "$1" ]; then
    echo "$1 is already set"
  else
    echo "setting $1"
    export $1=$2
  fi 
}

f_export_if_not_defined "HN_SREERAM_QA" "sreeram.qa.cashnetusa.com"
f_export_if_not_defined "HN_SREERAM_MXQA" "sreerammi.qa.cashnetusa.com"
f_export_if_not_defined "y" "2"
